package java_lab_04;
import java.util.Scanner;

public class Exercise_06_18 
{
	public static void main(String[] args)
	{
		Scanner input = new Scanner(System.in);
		String id =  input.nextLine();
		Valid_check(id);
	}
	public static boolean Valid_check(String id)
	{
		if(id.length() >  1)
			return true;
		else
			return false;
	}

}
